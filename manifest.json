{
  "manifest_version": 3,
  "name": "Github Show The Date",
  "description": "Github Show The Date.",
  "version": "1.0",
  "permissions": [
  ],
  "action": {
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://github.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
